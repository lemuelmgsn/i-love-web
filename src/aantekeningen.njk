<section>
<h1 class="section-title"> Aantekeningen </h1>

<ul role="list" class="articles-list">
{#  when u use your tags it will group it in a collection #}
    {%- for  post in collections.post -%}
    <li>
        <article class="snipppet"></assets>
        {# kijk in de post van collection.post > vind de data > pak de title  #}
            <h2> {{ post.data.title }}</h2>
            <p>door {{ post.data.presentator }}</p>
            {# | postDate past de addFilter toe zodat alleen de datum word getoont #}
            <p>{{ post.data.date | postDate }}</p>
            <p>{{ post.data.description }}</p>

    </li>
    {%- endfor -%}

</ul>

</section>

